<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>E-Way Bill Entry Form</title>
	<link rel="stylesheet" href="../stylesheets/entry.css">
	<link href="https://fonts.googleapis.com/css?family=Ubuntu" rel="stylesheet">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
</head>

<body>
	<script>
		function move(){
			let indus = document.querySelector(".indus").value;
			let datas = [{
				comp: 'TATA Tea',
				prod : 'Tea'
			},{
				comp: 'TMT Steels',
				prod : 'Robar steel'
			},{
				comp: 'Coorg Coffee',
				prod : 'Coffee'
			},{
				comp:'TATA Steels',
				prod :'Merchantbar'
			}];
			datas.forEach((data)=>{
				if(indus === data.comp){
					document.querySelector(".prod").value = data.prod;
				}
			});
		}
		
		
	
	</script>
	<div id="wrapper" style="background: url(../image/1.jpg);">
		<nav class="navbar navbar-inverse">
			<div class="container-fluid">
				<div class="navbar-header">
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a class="navbar-brand" href="/entry">GST</a>
				</div>
				<div class="collapse navbar-collapse" id="myNavbar">
					<ul class="nav navbar-nav">
						<li><a href="/entry">Home</a></li>
						<li class="active"><a href="/entry">Entry Form</a></li>
					</ul>
					<ul class="nav navbar-nav navbar-right">
						<li><a href="/Gst-Estimator">GST Estimatoy</a></li>
					</ul>
					<ul class="nav navbar-nav navbar-right">
						<li><a href="/logout"><span class="glyphicon glyphicon-log-in"></span> Logout</a></li>
					</ul>
				</div>
			</div>
		</nav>
		<div id="table">
			<center>
				<h3 style="color:white;"><b>E-WAY BILL</b></h3>
			</center>
			<center>
				<h5 style="color:white;"><b>ENTRY FORM</b></h5>
			</center>
		</div>
		<div class="content">
			<div id="signin">
				<form action="/entry" method="POST">
					<div class="form-group">
						<label for="name">GST IN</label>
						<input type="text" id="username" name="gstNo" required autofocus>
					</div>
					<div class="form-group">
						<label for="username">Industry Name</label>
						<input type="text" id="username" name="industryName" class="indus" onblur="move()" required>
					</div>
					<div class="form-group">
						<label for="username">Product Name</label>
						<input type="text" id="username" name="prod" class="prod" required>
					</div>
					<div class="form-group">
						<label for="username">Quantity</label>
						<input type="text" id="username" name="quant" required>
					</div>
					<div class="radio">
						<center>&nbsp;&nbsp;&nbsp;
							<select class="form-control" name="size">
								<option>Kg</option>
								<option>Tonnes</option>
								<option>Quintal</option>
							</select>
						</center>
					</div>
					<div class="form-group">
						<label for="username">HSN Code</label>
						<input type="text" id="username" name="hsn" required>
					</div>
					<div class="form-group">
						<label for="username">Transportation</label>
					</div>
					<div class="radio">
						<!-- &nbsp;&nbsp;&nbsp; -->
						<select class="form-control" name="wards">
							<option>Outwards</option>
							<option>Inwards</option>
						</select>
					</div>
					<div class="form-group">
						<label for="username">Mode</label>
					</div>
					<div class="radio">
						<!-- &nbsp;&nbsp;&nbsp; -->
						<select class="form-control" name="way">
							<option>Road</option>
							<option>Rail</option>
							<option>Air</option>
							<option>Ship</option>
						</select>
					</div>&nbsp;&nbsp;&nbsp;
					<div class="form-group">
						<label for="username">Vehicle Number</label>
						<input type="text" id="username" name="vechNo" required>
					</div>
					<div class="form-group">
						<label for="username">Place Of Delivery</label>
						<input type="text" id="username" name="pod" required>
					</div>
					<div class="form-group">
						<input type="submit" value="Generate" style="color:white;">
					</div>
				</form>
			</div>
		</div>
		<script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
</body>

</html>